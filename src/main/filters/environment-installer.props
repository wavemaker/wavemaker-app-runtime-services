#####################################################
#Config properties for desktop environment
#####################################################

#####################################################
#Db connection properties
#####################################################
wm.studio.db.driverClassName=org.hsqldb.jdbcDriver
wm.studio.hibernate.dialect=org.hibernate.dialect.HSQLDialect
wm.studio.db.connectionUrl=jdbc:hsqldb:file:wavemaker/db;create=true
wm.studio.db.username=SA
wm.studio.db.password=

#####################################################
#Db migration script properties
#####################################################
wm.studio.hibernate.hbm2ddl.auto=update

#####################################################
# static content & login Urls
#####################################################
wm.studio.static.content.url=/
wm.studio.login.url=login.html

