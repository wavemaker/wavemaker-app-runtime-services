#####################################################
#Config properties for saas staging environment
#####################################################

#####################################################
#Spring profile to be activated
#####################################################
wm.studio.spring.active.profiles=production

#####################################################
#Db connection properties
#####################################################
wm.studio.db.driverClassName=com.mysql.jdbc.Driver
wm.studio.hibernate.dialect=org.hibernate.dialect.MySQLDialect
wm.studio.db.connectionUrl=jdbc:mysql://localhost:3306/WMSTUDIO
wm.studio.db.username=root
wm.studio.db.password=pramati

#####################################################
#Db migration script properties
#####################################################
wm.studio.db.scripts.location=db/mysql
wm.studio.db.scripts.prefix=v
wm.studio.hibernate.hbm2ddl.auto=validate
wm.studio.db.should.migrate=true

#####################################################
# static content & login Urls
# properties name should not contain dot's(.) in it
#####################################################
wm_studio_static_content_url=//stage-wmstudio.s3.amazonaws.com/static/7.0
wm_studio_login_url_ui=https://stage-studio.wavemaker.com/login
wm_studio_login_url_backEnd=https://stage-studio.wavemaker.com/login
wm_studio_static_content_file_relative_location=dest
wm_studio_build_type=production

#####################################################
#Deploy Properties
#####################################################
wm_studio_deploy_host=staging.wavemakercloud.com
wm_studio_deploy_use_logged_in_user_token=true

#####################################################
#Saas Properties
#####################################################
wm_studio_saas_host=stage-studio.wavemaker.com
