<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<beans xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bean class="com.wavemaker.runtime.server.upload.FileUploadDownload" lazy-init="true" id="FileUploadDownload"/>
    <bean class="com.wavemaker.runtime.service.reflect.ReflectServiceWire" lazy-init="false">
        <property name="serviceId" value="FileUploadDownload"/>
        <property name="serviceType" ref="JavaService"/>
    </bean>
    <bean class="com.wavemaker.runtime.file.controller.RuntimeFileUploadDownloadController"/>
</beans>
